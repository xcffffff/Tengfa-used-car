<view class="usedCar">
    <view class="mask" hidden="{{maskFlag}}"></view>
    <view class="header">
        <view class="tab">
            <view class="tab_logo">
                <image class="img1" src="data:image/png;base64,"></image>
            </view>
            <view class="list {{show==index?'t':''}}" wx:for="{{titleNav}}" wx:key="index">{{item}}</view>
        </view>
        <view class="box">
            <view class="box1">
                <image src="data:image/png;base64,"></image>
                <text>全国</text>
            </view>
            <view class="box2">
                <image src="data:image/png;base64,"></image>
                <input type="text" placeholder="搜索品牌、车型" />
            </view>
        </view>
        <view class="navCar_ul">
            <view class="navCar_li" bindtap="navCar" data-msg="排序">
                <view>{{sortMsg}}</view>
                <image src="{{sortArrowsFlag? sortArrows:'../../static/icons/shangjiantou.png'}}"></image>
                <view class="sort" hidden="{{sortArrowsFlag}}">
                    <view class="sort_list" wx:for="{{sort}}" wx:key="index" bindtap="sortClick" data-sort="{{item.sort}}">
                        {{item.name}}
                    </view>
                </view>
            </view>
            <view class="navCar_li" bindtap="navCar" data-msg="品牌">
                <view>{{brandMsg}}</view>
                <image src="{{brandArrowsFlag? brandArrows:'../../static/icons/shangjiantou.png'}}"></image>
                <view class="brand_list" hidden="{{brandArrowsFlag}}">
                    <view class="brand_ziMu" wx:for="{{brandArr}}" wx:key="index">
                        <view class="ziMu_li">{{item.brand}}</view>
                        <block wx:for="{{item.childer}}" wx:for-item="ele" wx:for-index="i" wx:key="i">
                            <view class="Brand" bindtap="brandClick" data-id="{{ele.id}}">
                                {{ele.name}}
                            </view>
                        </block>
                    </view>
                </view>
            </view>
            <view class="navCar_li" bindtap="navCar" data-msg="价格">
                <view>{{priceMsg}}</view>
                <image src="{{priceArrowsFlag? priceArrows:'../../static/icons/shangjiantou.png'}}"></image>
                <view class="price_list" hidden="{{priceArrowsFlag}}">
                    <view class="price_li" wx:for="{{priceArr}}" wx:key="index" bindtap="priceClick" data-price="{{item.queryRules}}">{{item.label}}</view>
                </view>
            </view>
            <view class="navCar_li" bindtap="navCar" data-msg="车型">
                <view>{{carTypeMsg}}</view>
                <image src="{{carTypeArrowsFlag? carTypeArrows:'../../static/icons/shangjiantou.png'}}"></image>
                <view class="carType_list" hidden="{{carTypeArrowsFlag}}">
                    <view class="carType_li" wx:for="{{carTypeArr}}" wx:key="index" bindtap="carTypeClick" data-type="{{item.id}}">{{item.model}}</view>
                </view>
            </view>
            <view class="navCar_li" bindtap="navCar" data-msg="更多">
                <view>{{moreMsg}}</view>
                <image src="{{moreArrowsFlag? moreArrows:'../../static/icons/shangjiantou.png'}}"></image>
            </view>
        </view>
        <view class="select_ul">
            <view class="select_li" wx:for="{{selectArr}}" wx:key="index" bindtap="selec" data-num="{{item.saleType}}">
                {{item.name}}
            </view>
        </view>
    </view>
    <view class="carList">
        <view class="carList_ul">
            <view class="carList_li" wx:for="{{arr}}" wx:key="index">
                <image src="{{item.cover}}"></image>
                <view class="text">
                    <view class="title">{{item.carName}}</view>
                    <view class="dataTime">{{item.dateOfRegistration}} / {{item.mileage}}万公里</view>
                    <view class="price">{{item.currentPrice}}万</view>
                </view>
            </view>
        </view>
    </view>
</view>